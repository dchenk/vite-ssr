{
  "name": "vite-ssr-react",
  "version": "0.18.2",
  "description": "Vite utility for server side rendering",
  "scripts": {
    "dev": "npm-run-all dev:* --parallel dev:watch:* ",
    "dev:clean": "rm -rf node_modules/vite-ssr && mkdir node_modules/vite-ssr",
    "dev:copy": "cp package.json node_modules/vite-ssr-react/",
    "test": "uvu -r ts-node/register test/specs",
    "test:watch": "watchlist src test -- yarn test",
    "pretest": "npm run build:compile --outDir ./node_modules/vite-ssr-react && npm run dev:copy",
    "build": "npm-run-all build:*",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:copy": "cp README.md LICENSE dist/",
    "build:compile": "cd src && tsc",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dchenk/vite-ssr.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "ssr",
    "react"
  ],
  "author": "Fran Dios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dchenk/vite-ssr/issues"
  },
  "homepage": "https://github.com/dchenk/vite-ssr#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.js",
  "workspaces": [
    "examples/react-apollo",
    "test/fixtures/*"
  ],
  "peerDependencies": {
    "@vitejs/plugin-react": "^2.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.3.0",
    "react-ssr-prepass": "^1.5.0",
    "vite": "^3.1.3"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^3.0.0",
    "chalk": "^4.1.2",
    "connect": "^3.7.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/connect": "^3.4.34",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.4.7",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "playwright-chromium": "^1.13.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.8.4",
    "uvu": "^0.5.1",
    "watchlist": "^0.2.3"
  }
}
